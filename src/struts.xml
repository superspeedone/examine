<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- 更改struts2请求Action的后缀名，默认为action。若想去掉后缀，设为","即可
	<constant name="struts.action.extension" value=","></constant>--> 

	<package name="manageAction"  namespace="/manage" extends="struts-default">
      
		<action name="users" class="userManageAciton" >
			<result name="list">/WEB-INF/manage/userManage.jsp</result>
			<result name="addPage" >/WEB-INF/manage/userAdd.jsp</result>
			<result name="editPage" >/WEB-INF/manage/userEdit.jsp</result>
			<result name="delete" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">responseMsg</param>
			</result>
		</action>

	</package>
	
	<package name="studentAction"  namespace="/student" extends="struts-default">
      
	</package>
	
</struts>